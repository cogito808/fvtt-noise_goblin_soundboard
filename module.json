{
  "id": "fvtt-noise_goblin_soundboard",
  "title": "Noise Goblin Soundboard",
  "description": "Soundboard",
  "version": "1.0.0",
  "type": "module",
  "esmodules": [
    "scripts/soundboard.js",
    "scripts/soundboard-app.js"
  ],
  "styles": [
    "styles/style.css"
  ],
  "template": "templates/soundboard.html",
  "settings": [
    {
      "key": "soundboardDirectory",
      "name": "Soundboard Directory",
      "hint": "Path to the folder containing sound files.",
      "scope": "world",
      "config": true,
      "type": "String",
      "default": "assets/sounds"
    },
    {
      "key": "opacity",
      "name": "Soundboard Opacity",
      "hint": "Set the opacity of the soundboard window.",
      "scope": "client",
      "config": true,
      "type": "Number",
      "default": 1.0
    },
    {
      "key": "soundboardServerVolume",
      "name": "Server Volume",
      "hint": "Default volume level for sounds.",
      "scope": "world",
      "config": true,
      "type": "Number",
      "default": 1.0
    }
  ],
  "compatibility": {
    "minimum": "13.344",
    "verified": "13.347",
    "maximum": "13"
  },
  "manifest": "https://your-host.com/module.json",
  "download": "https://your-host.com/combat-sound.zip"
}
